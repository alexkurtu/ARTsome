<div class="container">
  <div class="gallery">
    <% @arts.each do |art| %>
      <div class="gallery-item img-h">
        <a href="<%= art_path(art.id) %>">
        <% unless art.picture.nil? %>
          <img src="<%= art.picture %>" alt="...">
        <% end %>
        </a>
      </div>
    <% end %>
  </div>
</div>